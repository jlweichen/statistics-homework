/********************************************************
Problem A - The Unmatched test of Guancho and Case
********************************************************/
proc freq;
tables guanza*case /chisq measures;
Title 'Problem A - Unmatched analysis of Guanza';


/********************************************************
Problem B - The Mantel_Haenszel test of Guancho and Case
********************************************************/
proc freq;
tables strat*guanza*case / noprint cmh;
*note the noprint option was put in to supress printing of
 the ~147 case-control strata tables;
Title 'Problem B - Mantel-Haenszel analysis of Guanza';

/********************************************************
